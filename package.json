    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "camelcase": "^5.3.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "rxjs": "^6.5.2",
    "tween-functions": "^1.2.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "yargs": "^13.3.0"
  },
  "peerDependencies": {
